/* pages/home/home.wxss */
page {
    background-color: rgb(226, 216, 216);
}

.top {
    width: 100%;
    position: fixed;
    top: 0;
    background-color:#fa2c19;
    z-index:100;
    text-align: center;
    color: #fff;
}
.top span {
    position: relative;
    top: 60rpx;
}
van-search {
    position: fixed;
    width: 100%;
    z-index: 99;
}
.wrapper {
    margin-top: 212rpx;
    width: 100%;
    background-image: url('https://m.360buyimg.com/mobilecms/s1125x939_jfs/t1/154351/13/5154/61941/5fa7ca47E0a8f501e/db68c387bcc1b5c3.jpg');
}
.ad-items-wrap {
    width: 90%;
    display: flex;
    margin: auto;
}
.ad-items {
    width: 100%;
    border-radius: 20rpx;
    transform: translateY(0);
    overflow: hidden;
    
}
.ad-items-img image{
    width: 100%;
    height: 100%;
}
.ad-items2 {
    width: 96%;
    margin: auto;
    display: flex;
    border-radius: 20rpx;
    transform: translateY(0);
    overflow: hidden;
}
.ad-items2 image {
    width: 100%;
    height: 200rpx;
}
.ad-items2-left {
    flex: 1;
}
.ad-items2-mid {
    flex: 2;
}
.ad-items2-right {
    flex: 1;
}
.box-list-wrap {
    width: 92%;
    margin: auto;
    background-color: rgb(226, 216, 216);
    border-radius: 20rpx;
    margin-top: 20rpx;
}
.box-list {
    width: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    margin-bottom: 20rpx;
}

.box-list-item {
    flex: 1;
    
}
.list-icon {
    width: 80rpx;
    height: 80rpx;
    margin: auto;
}
.list-icon image{
    width: 100%;
    height: 100%;
}
.box-list-item .title {
    color: #666666;
    font-size: 24rpx;
    text-align: center;
}
.seckill-wrap {
    width: 92%;
    margin: auto;
    margin-top: 10rpx;
    background-color: #fff;
    border-radius: 20rpx;
}
.title-wrap {
    width: 100%;
    height: 68rpx;
    display: inline-block;
    vertical-align: middle;
    line-height: 68rpx;
    background-image: url('data:image/png;base64,');
}
.seckill-title {
    width: 110rpx;
    float: left;
    color: #333333;
    font-size: 28rpx;
    text-align: center;
    margin-left: 20rpx;
}
.seckill-timer {
    float: left;
    height: 100%;
    display: flex;
    align-items: center;
    justify-content: center;
    margin-left: 20rpx;
}
.seckill-title-link {
    float: right;
    margin-right: 20rpx;
    color: #f23030;
    font-size: 24rpx;
}
.seckill-title-link span{
    width: 24rpx;
    height: 24rpx;
    display: inline-block;
    border-radius: 50%;
    background-color: #f23030;
    color: #fff;
    text-align: center;
    line-height: 24rpx;
}
.seckill-item-wrap {
    width: 100%;
    display: flex;
}
.seckill-item {
    flex: 1;
}
.seckill-item-img {
    width: 110rpx;
    height: 110rpx;
    margin: auto;

}
.seckill-item-img image {
    width: 100%;
    height: 100%;
}
.seckill-item-price {
    color: #f2270c;
    font-size: 26rpx;
    text-align: center;
    margin-top: 16rpx;
}
.seckill-item-oldPrice {
    color: #999999;
    font-size: 24rpx;
    text-align: center;
    text-decoration:line-through;
    margin-bottom: 16rpx;
}
.nav-class {
    background-color: #fff;
    margin-top: 20rpx;
}
